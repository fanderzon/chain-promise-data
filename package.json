  "description": "The purpose of this utility is to chain data from a chain of promises, into one custom object that looks the way you want it.",
  "main": "index.js",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "buble": "^0.15.2",
    "jest": "^20.0.4",
    "rollup": "^0.49.2",
    "rollup-plugin-buble": "^0.15.0"
  },
  "scripts": {
    "main": "dist/index.js",
    "module": "src/index.js",
    "test": "jest --coverage",
    "build": "rollup -c --environment format:cjs && rollup -c --environment format:es",
    "prepare": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanderzon/chain-promise-data.git"
